<body class="hold-transition login-page"
	style="background-color: #ffffff;">

	<div class="login-box">
		<!-- /.login-logo -->
		<div class="login-box-body">
			<form action="DSPLOGIN" method="post">

			<p class="login-box-msg" align="right">
				<a id="ufld:RESET_PASSWORD.AUTHORIZEDUSERS.EVENTS" class="white-text" >forgotten
					your password?</a>
			</p>

				<div class="form-group has-feedback">
					<input id="ufld:EMAIL_ADDRESS.AUTHORIZEDUSERS.EVENTS"
						class="form-control" placeholder="Email" type="email"><span
						class="glyphicon glyphicon-envelope form-control-feedback"></span>
				</div>
				<div class="form-group has-feedback">
					<input id="ufld:ENTERED_PASSWORD.AUTHORIZEDUSERS.EVENTS" class="form-control" placeholder="password" type="password" name="password">
<span toggle="#ufld:ENTERED_PASSWORD.AUTHORIZEDUSERS.EVENTS" class="fa fa-fw fa-eye field-icon toggle-password"></span>
<!--<span
						class="glyphicon glyphicon-lock form-control-feedback"></span>
-->
				</div>
				<div class="row">
					<div class="col-xs-6">
						<!--label class="btn white-text">
							style="background-color: rgb(238, 60, 66);"> <input
							type="checkbox" autocomplete="off">&nbsp;SAVE <span
							class="glyphicon glyphicon-ok"></span>
						</label-->
					</div>
					<!-- /.col -->
					<div class="col-xs-6">
						<button id="ufld:LOGMEIN.AUTHORIZEDUSERS.EVENTS" type="submit"
							class="btn btn-default btn-block btn-flat">Sign In</button>
					</div>
					<!-- /.col -->
				</div>


			</form>
		</div>

<!-- Disable until implemented

		<div class="social-auth-links text-center">
			<img src="/Events/images/divider.png" alt="" width='25%'>or
			sign in with <img src="/Events/images/divider.png" alt="" width='25%'>
			<br> <a href="#" class="btn  btn-social btn-facebook btn-flat" onClick="uniface.getInstance('DSPLOGIN').activate('tobeimplemented');return false;"><i
				class="fa fa-facebook"></i>Facebook</a> <a href="#"
